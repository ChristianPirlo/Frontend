@page "/"

@inject NavigationManager Navigation
@using System.ComponentModel.DataAnnotations
@using System.Net.Http
@inject HttpClient Http
@using System.Net.Http.Json
@using Microsoft.AspNetCore.Mvc
@using Microsoft.AspNetCore.Mvc.RazorPages

namespace BankSampahCRUD.Pages
{
    public class ReadModel : PageModel
    {
        public List<Sampah> SampahList { get; set; }

        public void OnGet()
        {
            // Mendapatkan daftar sampah dari database atau penyimpanan lainnya
            // Misalnya, menggunakan Entity Framework atau layanan lainnya
            SampahList = _sampahService.GetAllSampah();
        }
    }
}